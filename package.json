{
  "name": "@dcyfr/ai-nodejs-starter",
  "version": "1.0.0",
  "description": "DCYFR AI-powered Node.js & TypeScript starter template",
  "type": "module",
  "main": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "bin": {
    "dcyfr-ai": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "serve": "tsx src/server.ts",
    "serve:prod": "node dist/server.js",
    "cli": "tsx src/cli.ts",
    "cli:prod": "node dist/cli.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "dcyfr",
    "ai",
    "nodejs",
    "typescript",
    "starter",
    "template",
    "framework"
  ],
  "author": "DCYFR",
  "license": "MIT",
  "dependencies": {
    "@dcyfr/ai": "^1.0.1",
    "@types/express": "^5.0.6",
    "commander": "^14.0.2",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=24.13.0",
    "npm": ">=11.6.2"
  }
}
